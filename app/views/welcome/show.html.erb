
	<% if !current_user.diary %>
		<div id="form_intro"><%= @user.fname %>, to begin you must fill in your compu-pix form.  The form extracts numbers that may define you or be associated with you, or use the form to customize your Compu-Pix probabilities to any range you wish.</div>
		<div id='create_diary'><%= link_to "My Compu-pix Form", new_diary_path %></div>
		<% else %>
		<div id="profInfo">
			
			<div id="profName">Name: <%= @user.fname %></div>
			<div id="threeDigitProf">3 Digit Draws: <%= @user.three_digit_draws.count %></div>
			<div id="fourDigitProf">4 Digit Draws: <%= @user.four_digit_draws.count %></div>
			<div id="megaProf">Mega Draws: <%= @user.mega_draws.count %></div>


		</div>


	<% end %>





